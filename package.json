{
  "name": "react-template",
  "version": "1.0.0",
  "description": "",
  "jest": {
    "testEnvironment": "jsdom",
    "testURL": "http://localhost:4000/",
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/mocks/fileMock.ts",
      "\\.(css|less)$": "<rootDir>/tests/mocks/styleMock.ts"
    }
  },
  "scripts": {
    "build:client": "webpack --config webpack.config.production.ts",
    "build:server": "webpack --config webpack.config.server.ts --mode=production",
    "build": "npm run build:client && npm run build:server",
    "start:server": "nodemon dist/server/server.js",
    "start:client": "webpack serve --config webpack.config.dev.ts",
    "lint": "eslint --ext .ts,.tsx src",
    "lint-fix": "npm run lint -- --fix",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "author": "96tm",
  "private": true,
  "license": "ISC",
